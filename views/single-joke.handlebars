<article class="post">
  <div class="title">
    <h2>{{title}}</h2>
  </div>
  <div class="joke">{{joke_body}}</div>
  <div class="meta">
    by {{user.username}} on {{created_at}}
  </div>
  <div>
    <button type="button" class="upvote-btn">upvote</button>
  </div>
</article>
{{#if loggedIn}}
<script src="/javascript/upvote.js"></script>
{{/if}}